{
  "console.log()": {
    "prefix": "cl",
    "body": "console.log(${1:'$2', $3});\n"
  },
  "console.error()": {
    "prefix": "ce",
    "body": "console.error($1);\n"
  },
  "import from ''": {
    "prefix": "imp",
    "body": "import $2 from '$1';\n"
  },
  "import {} from ''": {
    "prefix": "imd",
    "body": "import { $2 } from '$1';\n"
  },
  "() => {}": {
    "prefix": "fn",
    "body": "const $1 = ${2:($3)} => $4\n"
  },
  "async () => {}": {
    "prefix": "afn",
    "body": "const $1 = async ${2:($3)} => $4\n"
  },
  "const = ": {
    "prefix": "c",
    "body": "const $1 = $2;\n"
  },
  "let = ": {
    "prefix": "l",
    "body": "let $1 = $2;\n"
  },
  "const =  {}": {
    "prefix": "obj",
    "body": "const $1 = {\n\t$2\n};\n"
  },
  "const =  []": {
    "prefix": "arr",
    "body": "const $1 = [\n\t$2\n];\n"
  },
  "const {} =": {
    "prefix": "des",
    "body": "const { $2 } = $1;\n"
  },
  "export default": {
    "prefix": "ed",
    "body": "export default $1;"
  },

  "describe": {
    "prefix": "desc",
    "body": ["describe('$1', () => {", "\t$0", "});", ""]
  },
  "testBlock": {
    "prefix": "test",
    "body": ["test('should $1', () => {", "\t$0", "});", ""]
  },
  "testAsyncBlock": {
    "prefix": "testa",
    "body": ["test('should $1', async () => {", "\t$0", "})", ""]
  },
  "itBlock": {
    "prefix": "it",
    "body": ["it('should $1', () => {", "\t$0", "})", ""]
  },
  "itAsyncBlock": {
    "prefix": "ita",
    "body": ["it('should $1', async () => {", "\t$0", "})", ""]
  },

  "import React from 'react'": {
    "prefix": "imr",
    "body": "import React from 'react';\n"
  },
  "import ReactDOM from 'react-dom'": {
    "prefix": "imr",
    "body": "import React-DOM from 'react-dom';\n"
  },
  "fragment": {
    "prefix": "fm",
    "body": "<>\n\t$1</>"
  },
  "util function": {
    "prefix": "ut",
    "body": ["export const $TM_FILENAME_BASE = () => {", "", "}"]
  }
}
