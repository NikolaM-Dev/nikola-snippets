{
  "Insert code fense": {
    "prefix": "c",
    "body": "```${1:language}\n${2}\n```"
  },

  "Insert link": {
    "prefix": "a",
    "body": "[${1:text}](${2:http://})"
  },

  "Insert image": {
    "prefix": "img",
    "body": "![${1:alt}](${2:http://})"
  },

  "Insert table": {
    "prefix": "table",
    "body": [
      "| ${1:Column1} | ${2:Column2 |",
      "| -----------: | ----------- |",
      "|   ${3:row1}  |   ${4:row1} |"
    ]
  }
}
